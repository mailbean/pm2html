{
  "name": "@mailbean/pm2html",
  "version": "0.0.32",
  "description": "ProseMirror to HTML",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "pub": "npm run build && npm version patch --force && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mailbean/pm2html.git"
  },
  "keywords": [
    "prosemirror",
    "html",
    "json"
  ],
  "author": "Philipp Viereck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mailbean/pm2html/issues"
  },
  "homepage": "https://github.com/mailbean/pm2html#readme",
  "devDependencies": {
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.15.5",
    "typescript": "^5.0.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
